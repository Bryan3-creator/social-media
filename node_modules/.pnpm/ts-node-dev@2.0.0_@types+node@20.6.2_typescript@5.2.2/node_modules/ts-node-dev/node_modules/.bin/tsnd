#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.6.2_typescript@5.2.2/node_modules/ts-node-dev/lib/node_modules:/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.6.2_typescript@5.2.2/node_modules/ts-node-dev/node_modules:/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.6.2_typescript@5.2.2/node_modules:/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.6.2_typescript@5.2.2/node_modules/ts-node-dev/lib/node_modules:/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.6.2_typescript@5.2.2/node_modules/ts-node-dev/node_modules:/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.6.2_typescript@5.2.2/node_modules:/data/data/com.termux/files/home/workspaces/social-media/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
